package com.demo.beans;

import java.util.Date;

public class SalariedEmp {
private double sal;
private double bonus;

public SalariedEmp() {
	super();
}

public SalariedEmp(String pname, Date pdate, String dept, String desg, double sal) {
	super("s", pname, pdate,dept, desg);
	this.sal = sal;
	this.bonus = sal*0.10;
}

public double getSal() {
	return sal;
}
public void setSal(double sal) {
	this.sal = sal;
}
public double getBonus() {
	return bonus;
}
public void setBonus(double bonus) {
	this.bonus = bonus;
}

@Override
public String toString() {
	return super.toString()+"SalariedEmp [sal=" + sal + ", bonus=" + bonus + "]";
}

}
